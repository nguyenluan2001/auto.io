{
    "root":true,
    "env": {
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "airbnb",
        "eslint:recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"

    },
    "plugins": ["prettier"],
    "rules": {
        "no-restricted-syntax": 0,
        "no-await-in-loop":0,
        "no-return-await":0
    }
}
